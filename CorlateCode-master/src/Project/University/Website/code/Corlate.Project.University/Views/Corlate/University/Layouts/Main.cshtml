@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using Sitecore.Mvc.Analytics.Extensions
@using Corlate.Feature.ProjectConfigurations.Models
@model RenderingModel
@{
    Layout = null;
    PageSettings pageSettings = new PageSettings(Model.PageItem);
    string faviconURL = (pageSettings != null && pageSettings.Identity != null) ? pageSettings.Identity.FaviconURL : string.Empty;
}

<!DOCTYPE html>
<html lang="@Sitecore.Context.Language.Name.ToLower()">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">   
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @Html.Partial("~/Views/Corlate/University/Sublayouts/Metadata/Metadata.cshtml")
      
    @if (pageSettings != null)
    {
        <link rel="shortcut icon" type="image/icon" href="@faviconURL" />       
    }

    <!-- CSS
    ================================================== -->
    <script src="~/assets/Corlate/University/js/jquery.1.11.1.min.js"></script>
    <link href="~/assets/Corlate/University/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/assets/Corlate/University/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/assets/Corlate/University/css/superslides.css" rel="stylesheet" />
    <link href="~/assets/Corlate/University/css/slick.css" rel="stylesheet" />
    <link href="~/assets/Corlate/University/css/jquery.circliful.css" rel="stylesheet" />
    <link href="~/assets/Corlate/University/css/animate.css" rel="stylesheet" />
    <link href="~/assets/Corlate/University/css/jquery.tosrus.all.css" rel="stylesheet" />
    <link href="~/assets/Corlate/University/css/themes/default-theme.css" rel="stylesheet" />
    <link href="~/assets/Corlate/University/css/custom.css" rel="stylesheet" />

    <!-- Google fonts -->
    <link href='http://fonts.googleapis.com/css?family=Merriweather' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Varela' rel='stylesheet' type='text/css'>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js
      https://oss.maxcdn.com/respond/1.4.2/respond.min.js
    <![endif]-->
    @Html.Sitecore().VisitorIdentification()
</head>
<body>
    <a class="scrollToTop" href="#"></a>
    <header id="header">
        @Html.Sitecore().Placeholder("header")
    </header>
    @Html.Sitecore().Placeholder("main")
    <footer id="footer">
        @Html.Sitecore().Placeholder("footer")
    </footer>   
    <!-- Javascript Files
    ================================================== -->
    <script src="~/assets/Corlate/University/js/jquery.1.11.1.min.js"></script>
    <script>jQuery.noConflict();</script>    
    <script src="~/assets/Corlate/University/js/queryloader2.min.js"></script>
    <script src="~/assets/Corlate/University/js/wow.min.js"></script>
    <script src="~/assets/Corlate/University/js/bootstrap.min.js"></script>
    <script src="~/assets/Corlate/University/js/slick.min.js"></script>
    <script src="~/assets/Corlate/University/js/jquery.easing.1.3.js"></script>
    <script src="~/assets/Corlate/University/js/jquery.animate-enhanced.min.js"></script>
    <script src="~/assets/Corlate/University/js/jquery.superslides.min.js"></script>
    <script src="~/assets/Corlate/University/js/jquery.circliful.min.js"></script>
    <script src="~/assets/Corlate/University/js/jquery.tosrus.min.all.js"></script>
    <script src="~/assets/Corlate/University/js/custom.js"></script>
    
    <!--===============================================
      Template Design By WpFreeware Team.
      Author URI : http://www.wpfreeware.com/
    ====================================================-->

</body>
</html>